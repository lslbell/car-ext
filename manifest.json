{
    "name": "MOT Quick search",
    "version": "1.0.0",
    "description": "Provides an on the page mot checker(UK)",
    "manifest_version": 3,
    "author": "Lewis Bell",
    "minimum_chrome_version": "88",
    "icons": {
        "16": "assets/icon.png",
        "32": "assets/icon.png",
        "48": "assets/icon.png",
        "128": "assets/icon.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "main.js"
    }
}